{
    "name": "spec-win512-hop256-chunks5s",
    "save_dir": "dataset/textureDataset/",
    "dataset": {
        "type": "CollectData",
        "args": {
            "path_to_dataset": [
                "dataset/textureDataset/audio"
            ],
            "extension": [
                "wav",
                "npy"
            ],
            "subset": "train"
        }
    },
    "transform1": {
        "type": "AudioRead",
        "args": {
            "sr": 22050
        }
    },
    "transform4": {
        "type": "Spectrogram",
        "args": {
            "sr": 22050,
            "n_fft": 512,
            "hop_size": 256,
            "spec_type": "lin"
        }
    },
    "transform5": {
        "type": "SpecChunking",
        "args": {
            "duration": 5,
            "sr": 22050,
            "hop_size": 256,
            "reverse": "False"
        }
    }
}