{   
    "name": "m256-.5s",
    "save_dir": "dataset/myAudioDataset/",
    "dataset": {
        "type": "CollectData",
        "args": {
            "path_to_dataset": ["dataset/textureDataset/audio"],
            "extension": ["wav", "npy"]
        }
    },
    "transform1": {
        "type": "AudioRead",
        "args": {
            "sr": 22050
        }
    },
    "transform4": {
        "type": "Spectrogram",
        "args": {
            "sr": 22050,
            "n_fft": 2048,
            "hop_size": 512,
            "spec_type": "lin"
        }
    },
    "transform5": {
        "type": "SpecChunking",
        "args": {
            "duration": 0.5,
            "sr": 22050,
            "hop_size": 512, 
            "reverse": "False"
        }
    }
}