{
    "name": "Conv1dSpecVAE",
    "n_gpu": 1,
    "arch": {
        "type": "Conv1dSpecVAE",
        "args": {
            "input_size": [
                64,
                10
            ],
            "latent_dim": 32,
            "is_featExtract": false,
            "n_convLayer": 2,
            "n_convChannel": [
                32,
                16
            ],
            "filter_size": [
                1,
                3
            ],
            "stride": [
                1,
                1
            ],
            "n_fcLayer": 1,
            "n_fcChannel": [
                256
            ]
        }
    },
    "data_loader": {
        "type": "CollectDataLoader",
        "args": {
            "data_dir": [
                "dataset/myAudioDataset/m64-5s"
            ],
            "batch_size": 32,
            "shuffle": true,
            "validation_split": 0.1,
            "num_workers": 0,
            "subset": "train"
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss": "vae_loss",
    "metrics": [
        "my_metric",
        "my_metric2"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 50,
            "gamma": 0.1
        }
    },
    "trainer": {
        "type": "Conv1dSpecVaeTrainer",
        "epochs": 10000,
        "save_dir": "saved/",
        "save_period": 10,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 1000,
        "tensorboardX": true
    }
}